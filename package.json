{
  "name": "@fto-consult/common",
  "version": "4.46.7",
  "description": "Un ensemble de bibliothèques et d'utilistaires communs pour le développement d'applications javascript",
  "main": "index.js",
  "scripts": {
    "test": "npx jest",
    "clear-jest": "npx jest --clearCache",
    "jest-clear": "npx run clear-jest",
    "unpublish": "npm -f unpublish @fto-consult/common",
    "delete-docs": "node ./deleteDocsFolder",
    "update-pdfmake": "node ./bin/update-pdfmake.js",
    "generate-jsonconfig": "node ./bin/generate-my-jsonconfig.js",
    "build-docs": "npm run delete-docs && npx jsdoc -r src  -c ./jsdoc.conf.json -d docs",
    "docs": "npm run build-docs",
    "update": "npm i js-base64@latest pdfmake@latest mongo-parse@latest @craftzdog/pouchdb-adapter-websql-core@latest pouchdb@latest pouchdb-authentication@latest pouchdb-erase@latest pouchdb-find@latest prop-types@latest qs@latest react-fast-compare@latest react-is@latest sanitize-filename@latest stable-hash@latest written-number@latest swr@latest && npm run update-pdfmake"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/borispipo/common.git"
  },
  "keywords": [
    "Bibliothèques",
    "utilitaires",
    "libraries",
    "utiles"
  ],
  "author": "Boris Fouomene",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/borispipo/common/issues"
  },
  "homepage": "https://github.com/borispipo/common#readme",
  "dependencies": {
    "@craftzdog/pouchdb-adapter-websql-core": "^7.2.3",
    "@fto-consult/node-utils": "^1.7.1",
    "circular-dependency-plugin": "^5.2.2",
    "color": "^4.2.3",
    "crypto-es": "^2.0.4",
    "eventsource": "^2.0.2",
    "install": "^0.13.0",
    "invariant": "^2.2.4",
    "js-base64": "^3.7.7",
    "moment": "^2.29.4",
    "mongo-parse": "^2.1.0",
    "npm": "^9.6.3",
    "pdfmake": "^0.2.10",
    "pouchdb": "^8.0.1",
    "pouchdb-authentication": "^1.1.3",
    "pouchdb-erase": "^1.0.2",
    "pouchdb-find": "^8.0.1",
    "prop-types": "^15.8.1",
    "qs": "^6.12.1",
    "react-fast-compare": "^3.2.2",
    "react-is": "^18.2.0",
    "sanitize-filename": "^1.6.3",
    "stable-hash": "^0.0.4",
    "swr": "^2.2.5",
    "written-number": "^0.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "jest": "^29.7.0"
  }
}
