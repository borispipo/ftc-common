{
    "files.associations": {
      "*.tsx": "typescriptreact",
      "*.ts": "typescriptreact"
    },
    "compilerOptions": {
      "strict": true,
      "baseUrl": ".",
      "resolveJsonModule": true,
      "experimentalDecorators": true,
      "lib": ["dom"],
      "paths": {
        "$capp/*" : ["src/common/app/*"],
        "$capp" : ["src/common/app"],
        "$capi/*" : ["src/common/api/*"],
        "$capi" : ["src/common/api"],
        "$fetch/*" : ["src/common/api/fetch/*"],
        "$fetch" : ["src/common/api/fetch"],
        "$ci18n/*" : ["src/common/i18n/*"],
        "$ci18n" : ["src/common/i18n"],
        "$clib/*" : ["src/common/lib/*"],
        "$clib" : ["src/common/lib"],
        "$cauth/*":["src/common/auth/*"],
        "$cauth":["src/common/auth"],
        "$cnavigation/*" : ["src/navigation/*"],
        "$cnavigation" : ["src/navigation"],
        "$cdimensions/*" : ["src/platform/dimensions/*"],
        "$cdimensions" : ["src/platform/dimensions"],
        "$cplatform/*" : ["src/platform/*"],
        "$cplatform" : ["src/platform"],
        "$dimensions/*" : ["src/platform/dimensions/*"],
        "$dimensions" : ["src/platform/dimensions"],
        "$platform/*" : ["src/platform/*"],
        "$platform" : ["src/platform"],
        "$cobservable" : ["src/lib/observable"],
        "$observable" : ["src/lib/observable"],
        "$cvalidator/*" : ["src/lib/validator/*"],
        "$cvalidator" : ["src/lib/validator"],
        "$validator/*" : ["src/lib/validator/*"],
        "$validator" : ["src/lib/validator"],
        "$cdate/*" : ["src/lib/date/*"],
        "$cdate" : ["src/lib/date"],
        "$date/*" : ["src/lib/date/*"],
        "$date" : ["src/lib/date"],
        "$crypto-js" : ["src/lib/crypto-js"],
        "$ccountries" : ["src/countries"],
        "$ccountries/*" : ["src/countries/*"],
        "$pouchdb/*" : ["src/pouchdb/*"],
        "$pouchdb" : ["src/pouchdb"],
        "$ctheme/*" : ["src/theme/*"],
        "$ctheme" : ["src/theme"],
        "$theme/*" : ["src/theme/*"],
        "$theme" : ["src/theme"],
        "$cnotify/*" : ["src/notify/*"],
        "$cnotify" : ["src/notify"],
        "$notify/*" : ["src/notify/*"],
        "$notify" : ["src/notify"],
        "$curi/*" : ["src/utils/uri/*"],
        "$curi" : ["src/utils/uri"],
        "$uri/*" : ["src/utils/uri/*"],
        "$uri" : ["src/utils/uri"],
        "$cutils/*" : ["src/utils/*"],
        "$cutils" : ["src/utils"],
        "$utils/*" : ["src/utils/*"],
        "$utils" : ["src/utils"],
        "$ccurrency/*" : ["src/lib/currency/*"],
        "$ccurrency" : ["src/lib/currency"],
        "$currency/*" : ["src/lib/currency/*"],
        "$currency" : ["src/lib/currency"],
        "$csession/*" : ["src/session/*"],
        "$csession" : ["src/session"],
        "$session/*" : ["src/session/*"],
        "$session" : ["src/session"],
        "$cbase64" : ["src/lib/base-64"],
        "$base64" : ["src/lib/base-64"],
        "$cactions" : ["src/actions"],
        "$actions" : ["src/actions"],        
        "$media" : ["src/media"],
        "$i18n/*" : ["src/i18n/*"],
        "$i18n" : ["src/i18n"],
        "$lib/*" : ["src/lib/*"],
        "$lib" : ["src/lib"],
        "$cfilters/*" : ["src/utils/filters/*"],
        "$cfilters" : ["src/utils/filters"],
        "$cprint/*" : ["src/print/*"],
        "$cprint" : ["src/print"],
        "$common/*"  : ["src/*"],
        "$csrc/*" : ["src/*"],
        "$cappConfig/*" : ["src/app/config/*"],
        "$cappConfig" : ["src/app/config"],
        "$appConfig/*" : ["src/app/config/*"],
        "$appConfig" : ["src/app/config"],
        "$swr/*" : ["src/swr/*"],
        "$swr" : ["src/swr"],
      }
    }
  }